{
  "name": "@dlarsson-se/cached-api",
  "version": "0.2.1",
  "description": "Cached API request package.",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "scripts": {
    "build": "tsc && rimraf ./dist/test && cp dist/* ../../frontend/app-template/node_modules/@dlarsson-se/cached-api/dist/",
    "build:clean": "rimraf ./dist",
    "build:publish": "cd ./dist && npm publish",
    "test": "jest --config jestconfig.json",
    "test:watch": "jest --watch",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlarssonse/cached-api.git"
  },
  "keywords": [
    "ng",
    "api"
  ],
  "author": "David Larsson <dlarsson.se@gmail.com> (https://www.dlarsson.se)",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.1.6",
    "zone.js": "^0.8.26"
  },
  "dependencies": {
    "@angular/common": "^7.1.1",
    "@angular/core": "^7.1.1",
    "@angular/http": "^7.1.1",
    "@angular/platform-browser": "^7.1.1",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.3"
  }
}
